<?xml version="1.0"?>
<openerp>
<data noupdate="1">
	<!--
	Sequences types for invoices - Out Invoice
	-->
	<record model="ir.sequence.type" id="seq_type_out_invoice_tax">
		<field name="name">Tax Invoice Out</field>
		<field name="code">invoice.out_invoice.tax</field>
	</record>
	<record model="ir.sequence.type" id="seq_type_out_invoice_retail">
		<field name="name">Retail Invoice Out</field>
		<field name="code">invoice.out_invoice.retail</field>
	</record>
	<record model="ir.sequence.type" id="seq_type_out_invoice_service">
		<field name="name">Service Invoice Out</field>
		<field name="code">invoice.out_invoice.service</field>
	</record>

	
	<!--
	Sequences types for invoices - Out Invoice
	-->
	<record model="ir.sequence" id="seq_out_invoice_tax">
		<field name="name">Tax Invoice Out</field>
		<field name="code">invoice.out_invoice.tax</field>
		<field name="padding" eval="3"/>
		<field name="prefix">%(year)s/</field>
	</record>
	<record model="ir.sequence" id="seq_out_invoice_retail">
		<field name="name">Retail Invoice Out</field>
		<field name="code">invoice.out_invoice.retail</field>
		<field name="padding" eval="3"/>
		<field name="prefix">%(year)s/</field>
	</record>
	<record model="ir.sequence" id="seq_out_invoice_service">
		<field name="name">Service Invoice Out</field>
		<field name="code">invoice.out_invoice.service</field>
		<field name="padding" eval="3"/>
		<field name="prefix">%(year)s/</field>
	</record>

</data>
</openerp>

